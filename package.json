{
  "name": "bank-api",
  "version": "1.0.0",
  "description": "bank application",
  "main": "server.js",
  "scripts": {
    "back:install": "cd backend && npm install && cd ..",
    "front:install": "cd frontend && npm install && cd ..",
    "install": "npm run back:install && npm run front:install",
    "doc": "cd frontend && npm run jsdoc && cd..",
    "server": " cd backend && npm run dev:server && cd ..",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laminemessaci/BankApi.git"
  },
  "author": "lamine messaci",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laminemessaci/BankApi/issues"
  },
  "homepage": "https://github.com/laminemessaci/BankApi#readme",
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
